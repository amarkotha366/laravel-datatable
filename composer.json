{
  "name": "hasib/laravel-datatables-oracle",
  "description": "jQuery DataTables API for Laravel 4|5",
  "keywords": [
    "laravel",
    "dataTables",
    "jquery"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Hashibul Hasan",
      "email": "amarkotha366@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "illuminate/database": "5.4.*|5.5.*|5.6.*|5.7.*",
    "illuminate/filesystem": "5.4.*|5.5.*|5.6.*|5.7.*",
    "illuminate/http": "5.4.*|5.5.*|5.6.*|5.7.*",
    "illuminate/support": "5.4.*|5.5.*|5.6.*|5.7.*",
    "illuminate/view": "5.4.*|5.5.*|5.6.*|5.7.*"
  },
  "require-dev": {
    "orchestra/testbench": "~3.5"
  },
  "suggest": {
    "hasib/laravel-datatables-buttons": "Plugin for server-side exporting of dataTables.",
    "hasib/laravel-datatables-html": "Plugin for server-side HTML builder of dataTables.",
    "hasib/laravel-datatables-fractal": "Plugin for server-side response using Fractal.",
    "hasib/laravel-datatables-editor": "Plugin to use DataTables Editor (requires a license)."
  },
  "autoload": {
    "psr-4": {
      "Hasib\\DataTables\\": "src/"
    },
    "files": [
      "src/helper.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Hasib\\DataTables\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "8.0-dev"
    },
    "laravel": {
      "providers": [
        "Hasib\\DataTables\\DataTablesServiceProvider"
      ],
      "aliases": {
        "DataTables": "Hasib\\DataTables\\Facades\\DataTables"
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
